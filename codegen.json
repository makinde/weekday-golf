{
  "schema": "https://dotherestxy.hasura.app/v1/graphql",
  "documents": ["./src/**/*.graphql"],
  "overwrite": true,
  "generates": {
    "./src/types.ts": {
      "plugins": [
        {
          "add": {
            "content": [
              "/**",
              " * This file is auto-generated by [GraphQL code generator](https://graphql-code-generator.com).",
              " * via the `generate` script in `package.json`. Do not edit this file, as it gets",
              " * overwritten before every build.",
              " * See `codegen.json` in the project root.",
              " */"
            ]
          }
        },
        "typescript",
        "typescript-operations",
        "typescript-graphql-request"
      ],
      "config": {
        "scalars": {
          "_int4": "number",
          "_text": "string",
          "date": "string",
          "numeric": "number",
          "point": "[number, number]",
          "jsonb": "{ [key: string]: any }",
          "timestamp": "string"
        }
      }
    }
  }
}