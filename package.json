{
  "name": "weekday-golf",
  "version": "0.0.1",
  "scripts": {
    "dev": "concurrently -n dev,generate -c \"bgCyan,bgMagenta\" \"next dev\" \"yarn generate --watch\"",
    "build": "next build",
    "start": "next start -p $PORT",
    "generate": "graphql-codegen",
    "analyze": "ANALYZE=true next build",
    "check": "npm run lint",
    "lint": "eslint . --report-unused-disable-directives",
    "lint:fix": "eslint . --fix",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "authors": [
    "makinde",
    "ericgio",
    "fzamore"
  ],
  "license": "MIT",
  "dependencies": {
    "@next/bundle-analyzer": "9.5.5",
    "bootstrap": "4.5.3",
    "classnames": "^2.2.6",
    "cross-fetch": "3.1.4",
    "date-fns": "2.16.1",
    "graphql": "15.4.0",
    "graphql-request": "^3.4.0",
    "lodash": "^4.17.20",
    "next": "10.0.3",
    "react": "^17.0.1",
    "react-bootstrap": "^1.4.0",
    "react-dom": "^17.0.1",
    "react-query": "3.13.12",
    "react-use": "17.1.1",
    "recharts": "2.0.9",
    "sass": "1.27.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.14.2",
    "@graphql-codegen/add": "2.0.2",
    "@graphql-codegen/cli": "1.21.4",
    "@graphql-codegen/typescript": "1.22.0",
    "@graphql-codegen/typescript-operations": "1.17.16",
    "@graphql-codegen/typescript-react-query": "1.3.0",
    "@types/lodash": "4.14.164",
    "@types/node": "14.14.5",
    "@types/react": "^17.0.5",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "4.6.0",
    "concurrently": "5.3.0",
    "eslint": "^7.11.0",
    "eslint-config-airbnb-typescript": "12.0.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "graphql-tag": "2.11.0",
    "husky": "^4.2.0",
    "lint-staged": "^10.0.2",
    "typescript": "4.0.5",
    "typescript-plugin-css-modules": "3.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run check"
    }
  },
  "lint-staged": {
    "*.js": "eslint --fix"
  },
  "installConfig": {
    "pnp": false
  }
}
